{
  "description": "Profile Web Extension",
  "manifest_version": 2,
  "name": "Profile Web Extension",
  "version": "1.0.0",
  "homepage_url": "https://github.com/tuqulore/web-ext-sample",
  "icons": {
    "48": "icons/48x48.png",
    "128": "icons/128x128.png"
  },
  "browser_action": {
    "default_icon": "icons/48x48.png",
    "default_title": "Profile Web Extension"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["contentScript.js"]
    }
  ],
  "permissions": ["activeTab"]
}
