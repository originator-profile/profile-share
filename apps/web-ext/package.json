{
  "name": "profile-web-ext",
  "description": "Profile Web Extensions",
  "version": "1.0.0",
  "author": "Kimiaki Kuno <knokmki612@gmail.com>",
  "bugs": {
    "url": "https://github.com/webdino/profile/issues"
  },
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@tsconfig/recommended": "1.0.1",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "@types/webextension-polyfill": "^0.8.3",
    "@typescript-eslint/eslint-plugin": "5.17.0",
    "@typescript-eslint/parser": "5.17.0",
    "@vitejs/plugin-react": "^1.3.0",
    "eslint": "8.12.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-plugin-tsc": "2.0.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.6.1",
    "rollup-plugin-copy": "^3.4.0",
    "typescript": "4.6.3",
    "vite": "^2.9.0",
    "vitest": "^0.8.1",
    "web-ext": "^6.8.0",
    "webextension-polyfill": "^0.9.0"
  },
  "engines": {
    "node": ">=14"
  },
  "homepage": "https://github.com/webdino/profile#readme",
  "keywords": [
    "webextension"
  ],
  "license": "MIT",
  "main": "src/main.ts",
  "repository": "git@github.com:webdino/profile.git",
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "packaging": "web-ext build --source-dir dist --overwrite-dest",
    "start:chromium": "web-ext run --source-dir dist --target=chromium",
    "start:firefox": "web-ext run --source-dir dist --target=firefox-desktop",
    "test": "vitest --passWithNoTests"
  }
}
