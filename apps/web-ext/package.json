    "@types/dompurify": "^2.3.4",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/rimraf": "^3.0.2",
    "@types/webextension-polyfill": "^0.9.0",
    "@webdino/profile-core": "*",
    "@webdino/profile-model": "*",
    "@webdino/profile-tsconfig": "*",
    "@webdino/profile-verify": "*",
    "autoprefixer": "^10.4.8",
    "clsx": "^1.2.1",
    "commander": "^9.4.0",
    "dompurify": "^2.4.0",
    "esbuild": "^0.15.6",
    "esbuild-copy-static-files": "^0.1.0",
    "eslint": "^8.23.0",
    "eslint-config-webdino-profile": "*",
    "jotai": "^1.8.1",
    "jsonld": "^8.1.0",
    "npm-run-all": "4.1.5",
    "playwright": "^1.25.1",
    "postcss": "^8.4.16",
    "postcss-load-config": "^4.0.1",
    "react-router-dom": "^6.3.0",
    "react-use": "^17.4.0",
    "rimraf": "^3.0.2",
    "swr": "^1.3.0",
    "tailwindcss": "^3.1.8",
    "typed-url-params": "^1.0.1",
    "typesafe-storage": "^1.0.9",
    "typescript": "4.8.2",
    "vitest": "^0.22.1",
    "web-ext": "^7.2.0",
    "webextension-polyfill": "^0.10.0"
  },
  "homepage": "https://github.com/webdino/profile#readme",
  "keywords": [
    "webextension"
  ],
  "license": "MIT",
  "main": "src/main.ts",
  "repository": "git@github.com:webdino/profile.git",
  "scripts": {
    "build": "run-s clean build:{esbuild,web-ext}",
    "build:esbuild": "node esbuild.build.js",
    "build:web-ext": "web-ext build --source-dir dist --overwrite-dest",
    "clean": "rimraf dist",
    "dev": "node esbuild.dev.js",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "start:chromium": "web-ext run --source-dir dist --target=chromium --no-reload",
    "start:firefox": "web-ext run --source-dir dist --target=firefox-desktop --no-reload",
    "e2e": "vitest --mode e2e run",
    "test": "vitest run"
  }
}
