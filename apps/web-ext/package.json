    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@headlessui/react": "^1.7.14",
    "@iconify/react": "^4.1.0",
    "@types/chrome": "^0.0.237",
    "@types/dompurify": "^3.0.2",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.4",
    "@types/rimraf": "^3.0.2",
    "@webdino/profile-core": "*",
    "@webdino/profile-model": "*",
    "@webdino/profile-tsconfig": "*",
    "@webdino/profile-ui": "*",
    "@webdino/profile-verify": "*",
    "autoprefixer": "^10.4.14",
    "clsx": "^1.2.1",
    "commander": "^10.0.1",
    "date-fns": "^2.30.0",
    "dompurify": "^3.0.3",
    "esbuild": "^0.17.19",
    "esbuild-copy-static-files": "^0.1.0",
    "eslint": "^8.41.0",
    "eslint-config-webdino-profile": "*",
    "jsonld": "^8.2.0",
    "just-extend": "^6.2.0",
    "npm-run-all": "4.1.5",
    "playwright": "^1.34.3",
    "postcss": "^8.4.24",
    "postcss-load-config": "^4.0.1",
    "react-router-dom": "^6.11.2",
    "react-use": "^17.4.0",
    "rimraf": "^5.0.1",
    "swr": "^2.1.5",
    "tailwind-config-webdino-profile": "*",
    "tailwindcss": "^3.3.2",
    "typed-url-params": "^1.0.1",
    "typescript": "^5.0.4",
    "vitest": "^0.31.1",
    "web-ext": "^7.6.2"
  },
  "homepage": "https://github.com/webdino/profile#readme",
  "keywords": [
    "webextension"
  ],
  "license": "MIT",
  "main": "src/main.ts",
  "repository": "git@github.com:webdino/profile.git",
  "scripts": {
    "build": "run-s clean build:{esbuild,web-ext}",
    "build:esbuild": "node esbuild.build.cjs",
    "build:web-ext": "web-ext build --source-dir dist --overwrite-dest",
    "clean": "rimraf dist",
    "dev": "node esbuild.dev.cjs",
    "lint": "eslint --fix .",
    "start:chromium": "web-ext run --source-dir dist --target=chromium --no-reload",
    "e2e": "vitest --mode e2e run",
    "test": "vitest run"
  }
}
