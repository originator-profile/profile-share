{
  "name": "@webdino/cat-extract",
  "description": "IAB Category definition extract tool",
  "version": "0.0.0",
  "private": true,
  "bin": "bin/run",
  "files": [
    "bin",
    "dist",
    "oclif.manifest.json"
  ],
  "scripts": {
    "build": "run-s build:{ts,cli} docs",
    "build:ts": "tsup src --clean",
    "build:cli": "oclif manifest",
    "dev": "dotenv -e .env.development run-p dev:{build}",
    "dev:build": "tsup src --watch --onSuccess 'run-p dev:{docs}'",
    "dev:docs": "run-s build:{cli} docs",
    "docs": "run-p docs:*",
    "docs:cli": "oclif readme",
    "e2e": "dotenv -e .env.development -- vitest --mode e2e run",
    "test": "vitest run",
    "lint": "eslint --fix .",
    "start": "bin/run start"
  },
  "dependencies": {
    "@oclif/core": "^2.8.5",
    "@oclif/plugin-help": "^5.2.9",
    "dotenv": "^16.0.3",
    "dotenv-expand": "^10.0.0",
    "playwright": "^1.34.3",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.0/xlsx-0.20.0.tgz"
  },
  "devDependencies": {
    "@webdino/profile-tsconfig": "*",
    "dotenv-cli": "^7.2.1",
    "esbuild-register": "^3.4.2",
    "eslint": "^8.41.0",
    "npm-run-all": "^4.1.5",
    "oclif": "^3.9.0",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4",
    "vitest": "^0.31.1"
  },
  "oclif": {
    "bin": "cat-extract",
    "commands": "dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ]
  }
}
