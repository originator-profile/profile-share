{
  "name": "@originator-profile/registry",
  "description": "Profile Registry API",
  "version": "0.2.0-beta.0",
  "private": false,
  "type": "commonjs",
  "bin": {
    "profile-registry": "bin/run"
  },
  "files": [
    "bin",
    "dist",
    "oclif.manifest.json"
  ],
  "scripts": {
    "build": "run-s build:{ts,db,ui,cli,api} docs",
    "build:ts": "tsup src --clean",
    "build:db": "shx cp -r ../../packages/registry-db/prisma dist/",
    "build:ui": "run-s ui:clean ui:build",
    "build:cli": "oclif manifest",
    "build:api": "bin/run openapi-gen",
    "dev": "dotenv -e .env.development -e .env.development.auth run-p dev:{docker,build,server:static}",
    "dev:docker": "docker compose --env-file .env.development up db minio minio_setup mailpit",
    "dev:build": "tsup src --watch --ignore-watch src/router.ts --onSuccess 'run-p dev:{docs,server}'",
    "dev:docs": "run-s build:{cli,api} docs",
    "dev:server": "run-s dev:{prisma,start}",
    "dev:server:static": "serve --listen 8081 --cors --config serve.json",
    "dev:prisma": "pnpm --filter @originator-profile/registry-db build",
    "dev:start": "bin/dev start --seed --schema=../../packages/registry-db/prisma/schema.prisma",
    "ui:build": "vite build",
    "ui:clean": "pnpm --filter @originator-profile/registry-ui clean",
    "docs": "run-p docs:*",
    "docs:cli": "oclif readme",
    "docs:api": "redocly build-docs dist/openapi.json -o ../../public/api/index.html",
    "e2e": "dotenv -e .env.development -- vitest --mode e2e run",
    "test": "vitest run",
    "lint": "eslint --fix .",
    "type-check": "tsc --noEmit",
    "start": "bin/run start"
  },
  "dependencies": {
    "@fastify/swagger-ui": "^5.2.2",
    "@fastify/vite": "^8.1.2",
    "@generouted/react-router": "^1.20.0",
    "@oclif/core": "^4.3.0",
    "@oclif/plugin-help": "^6.2.27",
    "@prisma/client": "^6.7.0",
    "@vitejs/plugin-react": "^4.4.1",
    "dotenv": "^16.5.0",
    "dotenv-expand": "^12.0.2",
    "fastify-auth0-verify": "^3.0.0",
    "jsdom": "^26.1.0",
    "jsonld": "^8.3.3",
    "playwright": "^1.52.0",
    "prisma": "^6.7.0",
    "vite": "^6.3.4"
  },
  "devDependencies": {
    "@fastify/autoload": "^6.3.0",
    "@fastify/basic-auth": "^6.2.0",
    "@fastify/cors": "^11.0.1",
    "@fastify/env": "^5.0.2",
    "@fastify/helmet": "^13.0.1",
    "@fastify/jwt": "^9.1.0",
    "@fastify/swagger": "^9.5.0",
    "@originator-profile/core": "workspace:*",
    "@originator-profile/cryptography": "workspace:*",
    "@originator-profile/model": "workspace:*",
    "@originator-profile/registry-db": "workspace:*",
    "@originator-profile/registry-service": "workspace:*",
    "@originator-profile/registry-ui": "workspace:*",
    "@originator-profile/securing-mechanism": "workspace:*",
    "@originator-profile/sign": "workspace:*",
    "@originator-profile/tsconfig": "workspace:*",
    "@originator-profile/ui": "workspace:*",
    "@originator-profile/verify": "workspace:*",
    "@redocly/cli": "^1.34.2",
    "@types/cli-progress": "^3.11.6",
    "@types/jsonld": "^1.5.15",
    "@types/uuid": "^10.0.0",
    "ajv": "^8.17.1",
    "cli-progress": "^3.12.0",
    "date-fns": "^4.1.0",
    "dotenv-cli": "^8.0.0",
    "esbuild-register": "^3.6.0",
    "eslint": "^9.25.1",
    "eslint-config-originator-profile": "workspace:*",
    "fastify": "^5.3.2",
    "fastify-http-errors-enhanced": "^6.0.1",
    "fastify-plugin": "^5.0.1",
    "globals": "^16.0.0",
    "globby": "^14.1.0",
    "http-errors-enhanced": "^3.0.2",
    "jose": "^6.0.10",
    "json-schema-to-ts": "^3.1.1",
    "just-extend": "^6.2.0",
    "just-merge": "^3.2.0",
    "just-omit": "^2.2.0",
    "msw": "^2.7.5",
    "npm-run-all2": "^7.0.2",
    "oclif": "^4.17.46",
    "secure-json-parse": "^4.0.0",
    "serve": "^14.2.4",
    "shx": "^0.4.0",
    "stdout-stderr": "^0.1.13",
    "tailwind-config-originator-profile": "workspace:*",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "uuid": "^11.1.0",
    "vitest": "^3.1.2"
  },
  "oclif": {
    "bin": "profile-registry",
    "commands": "dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ]
  }
}
