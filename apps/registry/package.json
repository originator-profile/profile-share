{
  "name": "@originator-profile/registry",
  "description": "Profile Registry API",
  "version": "0.0.1",
  "private": true,
  "bin": {
    "profile-registry": "bin/run"
  },
  "files": [
    "bin",
    "dist",
    "oclif.manifest.json"
  ],
  "scripts": {
    "build": "run-s build:{ts,db,ui,cli,api} docs",
    "build:ts": "tsup src --clean",
    "build:db": "shx cp -r ../../packages/registry-db/prisma dist/",
    "build:ui": "run-s ui:{clean,install}",
    "build:cli": "oclif manifest",
    "build:api": "bin/run openapi-gen",
    "dev": "dotenv -e .env.development run-p dev:{db,build}",
    "dev:db": "docker compose --env-file .env.development up db",
    "dev:build": "tsup src --watch --onSuccess 'run-p dev:{ui,docs,server}'",
    "dev:ui": "NODE_ENV=development run-s ui:{build,clean,install}",
    "dev:docs": "run-s build:{cli,api} docs",
    "dev:server": "run-s dev:{prisma,start}",
    "dev:prisma": "yarn workspace @originator-profile/registry-db build",
    "dev:start": "bin/dev start --seed --schema=../../packages/registry-db/prisma/schema.prisma",
    "ui:build": "yarn workspace @originator-profile/registry-ui build",
    "ui:clean": "shx rm -rf dist/public",
    "ui:install": "shx cp -r ../../packages/registry-ui/dist dist/public",
    "docs": "run-p docs:*",
    "docs:cli": "oclif readme",
    "docs:api": "widdershins dist/openapi.json --omitHeader --language_tabs shell:Shell --outfile ../../docs/registry/assets/api.md",
    "docs:assets": "shx cp dist/openapi.json ../../docs/registry/assets/openapi.json",
    "e2e": "dotenv -e .env.development -- vitest --mode e2e run",
    "test": "vitest run",
    "lint": "eslint --fix .",
    "start": "bin/run start"
  },
  "dependencies": {
    "@oclif/core": "^2.8.11",
    "@oclif/plugin-help": "^5.2.11",
    "@prisma/client": "^4.16.2",
    "dotenv": "^16.3.1",
    "dotenv-expand": "^10.0.0",
    "jsonld": "^8.2.0",
    "metascraper": "^5.34.8",
    "metascraper-author": "^5.34.7",
    "metascraper-date": "^5.34.7",
    "metascraper-description": "^5.34.7",
    "metascraper-image": "^5.34.7",
    "metascraper-title": "^5.34.7",
    "playwright": "^1.35.1",
    "prisma": "^4.16.2",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.0/xlsx-0.20.0.tgz"
  },
  "devDependencies": {
    "@fastify/autoload": "^5.7.1",
    "@fastify/basic-auth": "^5.0.0",
    "@fastify/cors": "^8.3.0",
    "@fastify/env": "^4.2.0",
    "@fastify/helmet": "^11.0.0",
    "@fastify/swagger": "^8.7.0",
    "@fastify/swagger-ui": "^1.9.2",
    "@types/jsonld": "^1.5.9",
    "@types/serve-handler": "^6.1.1",
    "@types/uuid": "^9.0.2",
    "@originator-profile/core": "*",
    "@originator-profile/model": "*",
    "@originator-profile/registry-db": "*",
    "@originator-profile/registry-service": "*",
    "@originator-profile/registry-ui": "*",
    "@originator-profile/sign": "*",
    "@originator-profile/tsconfig": "*",
    "@originator-profile/verify": "*",
    "ajv": "^8.12.0",
    "cross-fetch": "^4.0.0",
    "date-fns": "^2.30.0",
    "dotenv-cli": "^7.2.1",
    "esbuild-register": "^3.4.2",
    "eslint": "^8.44.0",
    "eslint-config-originator-profile": "*",
    "fastify": "^4.19.2",
    "fastify-http-errors-enhanced": "^4.1.0",
    "globby": "^13.2.1",
    "json-schema-to-ts": "^2.9.1",
    "just-omit": "^2.2.0",
    "npm-run-all": "^4.1.5",
    "oclif": "^3.9.1",
    "serve-handler": "^6.1.5",
    "shx": "^0.3.4",
    "stdout-stderr": "^0.1.13",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6",
    "uuid": "^9.0.0",
    "vitest": "^0.32.4",
    "widdershins": "^4.0.1"
  },
  "oclif": {
    "bin": "profile-registry",
    "commands": "dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ]
  }
}
