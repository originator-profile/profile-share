{
  "name": "@originator-profile/registry",
  "description": "Profile Registry API",
  "version": "0.0.1",
  "private": true,
  "bin": {
    "profile-registry": "bin/run"
  },
  "files": [
    "bin",
    "dist",
    "oclif.manifest.json"
  ],
  "scripts": {
    "build": "run-s build:{ts,db,ui,cli,api} docs",
    "build:ts": "tsup src --clean",
    "build:db": "shx cp -r ../../packages/registry-db/prisma dist/",
    "build:ui": "run-s ui:clean ui:build",
    "build:cli": "oclif manifest",
    "build:api": "bin/run openapi-gen",
    "dev": "dotenv -e .env.development run-p dev:{docker,build}",
    "dev:docker": "docker compose --env-file .env.development up db minio minio_setup",
    "//TODO:gh-770:dev:docker": "docker compose --env-file .env.development up db mailpit",
    "dev:build": "tsup src --watch --ignore-watch src/router.ts --onSuccess 'run-p dev:{docs,server}'",
    "dev:docs": "run-s build:{cli,api} docs",
    "dev:server": "run-s dev:{prisma,start}",
    "dev:prisma": "yarn workspace @originator-profile/registry-db build",
    "dev:start": "bin/dev start --seed --schema=../../packages/registry-db/prisma/schema.prisma",
    "ui:build": "vite build",
    "ui:clean": "yarn workspace @originator-profile/registry-ui clean",
    "docs": "run-p docs:*",
    "docs:cli": "oclif readme",
    "docs:api": "widdershins dist/openapi.json --omitHeader --language_tabs shell:Shell --outfile ../../docs/registry/assets/api.md",
    "docs:assets": "shx cp dist/openapi.json ../../docs/registry/assets/openapi.json",
    "e2e": "dotenv -e .env.development -- vitest --mode e2e run",
    "test": "vitest run",
    "lint": "eslint --fix .",
    "start": "bin/run start"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.420.0",
    "@fastify/vite": "^4.0.0",
    "@oclif/core": "^2.15.0",
    "@oclif/plugin-help": "^5.2.19",
    "@prisma/client": "^5.3.1",
    "@vitejs/plugin-react": "^4.1.0",
    "dotenv": "^16.3.1",
    "dotenv-expand": "^10.0.0",
    "fastify-auth0-verify": "^1.2.0",
    "image-size": "^1.0.2",
    "jsonld": "^8.3.1",
    "metascraper": "^5.37.1",
    "metascraper-author": "^5.37.1",
    "metascraper-date": "^5.37.1",
    "metascraper-description": "^5.37.1",
    "metascraper-image": "^5.37.1",
    "metascraper-title": "^5.37.1",
    "playwright": "^1.38.1",
    "prisma": "^5.3.1",
    "vite": "^4.4.9",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.0/xlsx-0.20.0.tgz"
  },
  "devDependencies": {
    "@fastify/autoload": "^5.7.1",
    "@fastify/basic-auth": "^5.0.0",
    "@fastify/cors": "^8.4.0",
    "@fastify/env": "^4.2.0",
    "@fastify/helmet": "^11.1.1",
    "@fastify/swagger": "^8.10.1",
    "@fastify/swagger-ui": "^1.9.3",
    "@originator-profile/core": "*",
    "@originator-profile/model": "*",
    "@originator-profile/registry-db": "*",
    "@originator-profile/registry-service": "*",
    "@originator-profile/registry-ui": "*",
    "@originator-profile/sign": "*",
    "@originator-profile/tsconfig": "*",
    "@originator-profile/verify": "*",
    "@types/jsonld": "^1.5.10",
    "@types/uuid": "^9.0.4",
    "ajv": "^8.12.0",
    "date-fns": "^2.30.0",
    "dotenv-cli": "^7.3.0",
    "esbuild-register": "^3.5.0",
    "eslint": "^8.50.0",
    "eslint-config-originator-profile": "*",
    "fastify": "^4.23.2",
    "fastify-http-errors-enhanced": "^4.1.0",
    "globby": "^13.2.2",
    "jose": "^4.14.6",
    "json-schema-to-ts": "^2.9.2",
    "just-flush": "^2.3.0",
    "just-omit": "^2.2.0",
    "msw": "^1.3.1",
    "npm-run-all": "^4.1.5",
    "oclif": "^3.17.1",
    "shx": "^0.3.4",
    "stdout-stderr": "^0.1.13",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "uuid": "^9.0.1",
    "vitest": "^0.34.5",
    "widdershins": "^4.0.1"
  },
  "oclif": {
    "bin": "profile-registry",
    "commands": "dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ]
  }
}
