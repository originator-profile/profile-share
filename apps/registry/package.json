{
  "name": "@webdino/profile-registry",
  "description": "Profile Registry API",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "run-s build:{db,app,docs}",
    "build:db": "yarn workspace @webdino/profile-registry-db build",
    "build:app": "tsup src --clean --onSuccess 'node dist/openapi-gen.js'",
    "build:docs": "run-p build:docs:*",
    "build:docs:gen": "widdershins dist/openapi.json --omitHeader --language_tabs shell:Shell --outfile ../../docs/registry/assets/api.md",
    "build:docs:assets": "shx cp dist/openapi.json ../../docs/registry/assets/openapi.json",
    "dev": "dotenv -e $PWD/.env.development run-p dev:*",
    "dev:db": "run-p dev:db:*",
    "dev:db:init": "docker compose --env-file .env.development up db",
    "dev:db:gen": "yarn workspace @webdino/profile-registry-db dev",
    "dev:db:seed": "yarn workspace @webdino/profile-registry-db seed",
    "dev:app": "NODE_ENV=development tsup src --watch --onSuccess 'npm start'",
    "dev:docs": "nodemon --watch dist/openapi.json --exec 'npm run build:docs'",
    "e2e": "dotenv -e $PWD/.env.development start-test dev http-get://:8080/ e2e:run",
    "e2e:run": "vitest --mode e2e run",
    "lint": "eslint --fix .",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "@prisma/client": "^3.12.0"
  },
  "devDependencies": {
    "@types/jsonld": "^1.5.6",
    "@webdino/profile-model": "*",
    "@webdino/profile-registry-db": "*",
    "@webdino/profile-registry-service": "*",
    "@webdino/profile-tsconfig": "*",
    "ajv": "^8.11.0",
    "cross-fetch": "^3.1.5",
    "dotenv-cli": "^5.1.0",
    "eslint": "^8.13.0",
    "eslint-config-webdino-profile": "*",
    "eslint-plugin-filenames": "^1.3.2",
    "fastify": "^3.28.0",
    "fastify-autoload": "^3.11.0",
    "fastify-cors": "^6.0.3",
    "fastify-env": "^2.1.1",
    "fastify-http-errors-enhanced": "^3.0.6",
    "fastify-swagger": "^5.1.0",
    "json-schema-to-ts": "^2.1.1",
    "jsonld": "^5.2.0",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "pino-pretty": "^7.6.1",
    "shx": "^0.3.4",
    "start-server-and-test": "^1.14.0",
    "tsup": "^5.12.4",
    "vitest": "^0.9.3",
    "vitest-mock-extended": "^0.1.3",
    "widdershins": "^4.0.1"
  }
}
