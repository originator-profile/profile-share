{
  "name": "@originator-profile/opvc",
  "version": "0.3.0-beta.3",
  "private": false,
  "type": "module",
  "files": [
    "bin",
    "dist"
  ],
  "bin": "bin/run.js",
  "exports": {
    "types": "./dist/index.d.ts",
    "default": "./dist/index.js"
  },
  "oclif": {
    "bin": "opvc",
    "commands": "dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ]
