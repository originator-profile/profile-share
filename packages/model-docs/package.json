{
  "name": "@originator-profile/model-docs",
  "version": "0.0.13",
  "private": true,
  "scripts": {
    "build": "run-s build:schema build:docs",
    "build:schema": "node -r esbuild-register scripts/schema-gen.mts",
    "build:docs": "run-s build:docs:clean build:docs:gen",
    "build:docs:clean": "rimraf ../../docs/spec/model/assets",
    "build:docs:gen": "jsonschema2md --input dist --out ../../docs/spec/model/assets --schema-out ../../docs/spec/model/assets",
    "test": "markdown-link-check --config ./markdown-link-check.config.json ../../docs/spec/model/assets/*.md",
    "lint": "eslint --fix ."
  },
  "devDependencies": {
    "@adobe/jsonschema2md": "^8.0.2",
    "@originator-profile/model": "workspace:*",
    "@originator-profile/tsconfig": "workspace:*",
    "esbuild-register": "^3.5.0",
    "eslint": "^9.7.0",
    "eslint-config-originator-profile": "workspace:*",
    "just-kebab-case": "^4.2.0",
    "markdown-link-check": "^3.12.2",
    "npm-run-all2": "^6.2.0",
    "rimraf": "^6.0.0"
  }
}
