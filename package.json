{
  "name": "@originator-profile/profile",
  "description": "Originator Profile",
  "homepage": "https://docs.originator-profile.org",
  "repository": "https://github.com/originator-profile/profile",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel --env-mode=loose",
    "e2e": "start-test dev http-get://localhost:8080/ e2e:run",
    "e2e:run": "turbo run e2e --concurrency=1 --filter=!@originator-profile/wordpress",
    "wordpress:e2e": "pnpm --filter=@originator-profile/wordpress e2e",
    "test": "turbo run test --continue",
    "lint": "turbo run lint --continue",
    "type-check": "turbo run type-check --continue",
    "format": "prettier --write .",
    "update": "run-s update:pnpm update:dedupe",
    "update:pnpm": "pnpm --recursive update --latest",
    "update:dedupe": "pnpm dedupe",
    "docs": "run-s build docs:{ts,build}",
    "docs:ts": "typedoc",
    "docs:build": "docusaurus build --out-dir pages",
    "docs:preview": "docusaurus start",
    "start": "pnpm --filter=@originator-profile/registry run --sequential \"/^build:ui$|^start$/\""
  },
  "devDependencies": {
    "@docusaurus/core": "^3.6.0",
    "@docusaurus/preset-classic": "^3.6.0",
    "@docusaurus/theme-mermaid": "^3.6.0",
    "@docusaurus/types": "^3.6.0",
    "@easyops-cn/docusaurus-search-local": "^0.46.0",
    "@lerna-lite/cli": "^3.10.0",
    "@lerna-lite/version": "^3.10.0",
    "@mdx-js/react": "^3.1.0",
    "npm-run-all2": "^7.0.1",
    "prettier": "^3.3.3",
    "react": "18.3.1",
    "start-server-and-test": "^2.0.8",
    "turbo": "2.3.3",
    "typedoc": "^0.27.0"
  },
  "engines": {
    "node": "^20.18.0 || ^22.2.0"
  },
  "packageManager": "pnpm@9.14.4"
}
