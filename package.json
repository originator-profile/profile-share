{
  "name": "@webdino/profile",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "e2e": "turbo run e2e",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "format": "prettier --ignore-path .gitignore --write . '!docs/**/assets'"
  },
  "devDependencies": {
    "prettier": "^2.5.1",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "yarn@1.22.18"
}
