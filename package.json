{
  "name": "@webdino/profile",
  "description": "Originator Profile",
  "homepage": "https://profile-docs.pages.dev",
  "repository": "https://github.com/webdino/profile",
  "version": "0.0.1",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "e2e": "start-test dev http-get://:8080/ e2e:run",
    "e2e:run": "turbo run e2e --parallel",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "format": "prettier --write .",
    "update": "run-s update:{npm,dedupe}",
    "update:npm": "npm-check-updates --deep --upgrade",
    "update:dedupe": "yarn-deduplicate",
    "docs": "run-s build docs:{ts,build}",
    "docs:ts": "typedoc",
    "docs:build": "docusaurus build --out-dir pages",
    "docs:preview": "docusaurus start",
    "start": "profile-registry start"
  },
  "devDependencies": {
    "@docusaurus/core": "^2.4.1",
    "@docusaurus/preset-classic": "^2.4.1",
    "@docusaurus/types": "^2.4.1",
    "mdx-mermaid": "^1.3.2",
    "mermaid": "^10.2.4",
    "npm-check-updates": "^16.10.13",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "start-server-and-test": "^2.0.0",
    "turbo": "latest",
    "typedoc": "^0.24.8",
    "yarn-deduplicate": "^6.0.2"
  },
  "engines": {
    "node": "^16.17.0 || ^18.7.0"
  },
  "packageManager": "yarn@1.22.19"
}
