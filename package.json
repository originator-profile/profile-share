{
  "name": "@originator-profile/profile",
  "description": "Originator Profile",
  "homepage": "https://docs.originator-profile.org",
  "repository": "https://github.com/originator-profile/profile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "e2e": "start-test dev http-get://localhost:8080 e2e:run",
    "e2e:run": "turbo run e2e --concurrency=1",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "format": "prettier --write .",
    "update": "run-s update:pnpm update:dedupe",
    "update:pnpm": "pnpm --recursive update --latest",
    "update:dedupe": "pnpm dedupe",
    "docs": "run-s build docs:{ts,build}",
    "docs:ts": "typedoc",
    "docs:build": "docusaurus build --out-dir pages",
    "docs:preview": "docusaurus start",
    "start": "profile-registry start --schema=packages/registry-db/prisma/schema.prisma"
  },
  "devDependencies": {
    "@docusaurus/core": "^3.0.1",
    "@docusaurus/preset-classic": "^3.0.1",
    "@docusaurus/theme-mermaid": "^3.0.1",
    "@docusaurus/types": "^3.0.1",
    "@mdx-js/react": "^3.0.0",
    "npm-check-updates": "^16.14.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.0",
    "react": "18.2.0",
    "start-server-and-test": "^2.0.3",
    "turbo": "1.11.0",
    "typedoc": "^0.25.4"
  },
  "engines": {
    "node": "^18.18.0 || ^20.5.1"
  },
  "packageManager": "pnpm@8.11.0"
}
