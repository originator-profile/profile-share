{
  "name": "@webdino/profile",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "e2e": "start-test dev http-get://:8080/ e2e:run",
    "e2e:run": "turbo run e2e --parallel",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "format": "prettier --ignore-path .gitignore --write . '!docs/**/assets'",
    "update": "run-s update:{npm,dedupe}",
    "update:npm": "npm-check-updates --deep --upgrade",
    "update:dedupe": "yarn-deduplicate",
    "start": "profile-registry start"
  },
  "devDependencies": {
    "npm-check-updates": "^14.1.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "start-server-and-test": "^1.14.0",
    "turbo": "latest",
    "yarn-deduplicate": "^5.0.0"
  },
  "engines": {
    "node": "^16.15.0"
  },
  "packageManager": "yarn@1.22.18"
}
