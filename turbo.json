{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        "dist/**",
        "dist-*/**",
        "web-ext-artifacts/**",
        "web-ext-artifacts-*/**"
      ]
    },
    "dev": {
      "cache": false
    },
    "e2e": {
      "cache": false
    },
    "test": {
      "dependsOn": ["build"],
      "outputs": []
    },
    "lint": {
      "dependsOn": ["@originator-profile/registry-db#build"],
      "outputs": []
    }
  }
}
